[user]
	name = poximy
	email = 76846708+poximy@users.noreply.github.com
[color]
	ui = auto
[core]
	editor = nvim
	autocrlf = input
[init]
	defaultBranch = main
[diff]
 	tool = nvim -d
[alias]
	# Staging
	s = status -s

	# log
	og = log --oneline --graph
	last = log -1 HEAD --stat
	oneline = log --oneline

	# branch
	del = branch -D

	# misc
	undo = reset --soft HEAD^
